<%=form_with model: @administrator do |f|%>
<%=f.label :name%>
<%=f.text_field :name%>
<br/>
<br/>
<%=f.label :user_name%>
<%=f.text_field :user_name%>
<br/>
<br/>
<%=f.label :password%>
<%=f.text_field :password%>
<br/>
<br/>
<%=f.label :status%>
<%=f.text_field :status%>
<br/>
<br/>
<%=f.submit "Submit"%>
<%end%>




